<style>
    @import url("https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;700;800&display=swap");
    * {
        margin: 0;
        padding: 0;
        box-sizing: border-box;
        font-family: "Poppins", sans-serif;
    }

    body {
        display: flex;
        justify-content: center;
        align-items: center;
        flex-wrap: wrap;
        min-height: 100vh;
    }

    body .container {
        display: flex;
        justify-content: center;
        align-items: center;
        flex-wrap: wrap;
        max-width: 1200px;
    }

    body .container .card {
        position: relative;
        min-width: 320px;
        height: 440px;
        box-shadow: inset 5px 5px 5px rgba(0, 0, 0, 0.2),
            inset -5px -5px 15px rgba(255, 255, 255, 0.1),
            5px 5px 15px rgba(0, 0, 0, 0.3), -5px -5px 15px rgba(255, 255, 255, 0.1);
        border-radius: 15px;
        margin: 30px;
        transition: 0.5s;
    }

    body .container .card:nth-child(1) .box .content a {
        background: red;
    }

    body .container .card:nth-child(2) .box .content a {
        background: orange;
    }

    body .container .card:nth-child(3) .box .content a {
        background: rgb(196, 196, 1);
    }
    
    body .container .card:nth-child(4) .box .content a {
        background: green;
    }

    body .container .card:nth-child(5) .box .content a {
        background: blue;
    }

    body .container .card:nth-child(6) .box .content a {
        background: rgb(244, 140, 157);
    }

    body .container .card:nth-child(7) .box .content a {
        background: purple;
    }

    body .container .card:nth-child(8) .box .content a {
        background: indigo;
    }

    body .container .card:nth-child(9) .box .content a {
        background: violet;
    }

    body .container .card:nth-child(10) .box .content a {
        background: brown;
    }

    body .container .card:nth-child(11) .box .content a {
        background: gray;
    }

    body .container .card:nth-child(12) .box .content a {
        background: black;
    }


    body .container .card .box {
        position: absolute;
        top: 15px;
        left: 13px;
        right: 20px;
        bottom: 20px;
        background: #2a2b2f;
        border-radius: 15px;
        display: flex;
        justify-content: center;
        align-items: center;
        overflow: hidden;
        transition: 0.5s;
    }

    body .container .card .box:hover {
        transform: translateY(-50px);
    }

    body .container .card .box:before {
        content: "";
        position: absolute;
        top: 0;
        left: 0;
        width: 50%;
        height: 100%;
        background: rgba(255, 255, 255, 0.03);
    }

    body .container .card .box .content {
        padding: 20px;
        display: flex;
        text-align: center;
        justify-content: center;
        height: 400px;
        width: 300px;
    }

    body .container .card .box .content h2 {
        position: absolute;
        top: -10px;
        right: 30px;
        font-size: 8rem;
        color: rgba(255, 255, 255, 0.1);
    }

    body .container .card .box .content h3 {
        font-size: 1.8rem;
        color: #fff;
        z-index: 1;
        transition: 0.5s;
        position: absolute;
        top: 110px;
    }

    body .container .card .box .content p {
        font-size: 1rem;
        font-weight: 300;
        color: rgba(255, 255, 255, 0.9);
        z-index: 1;
        transition: 0.5s;
        margin-top: 30px;
        position: absolute;
        top: 140px;
        left: 10px;
    }

    body .container .card .box .content a {
        position: relative;
        display: inline-block;
        padding: 8px 20px;
        background: black;
        border-radius: 5px;
        text-decoration: none;
        color: white;
        margin-top: 20px;
        box-shadow: 0 10px 20px rgba(0, 0, 0, 0.2);
        transition: 0.5s;
        position: absolute;
        top: 280px;
        left: 78px;
    }
    body .container .card .box .content a:hover {
        box-shadow: 0 10px 20px rgba(0, 0, 0, 0.6);
        background: #fff;
        color: #000;
    }
    .centered {
      width: 100%;
      display: flex;
      justify-content: center;
    }
</style>







